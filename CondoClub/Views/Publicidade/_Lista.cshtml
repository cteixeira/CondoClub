@model IEnumerable<CondoClub.Web.Models.Publicidade>

@foreach (var item in Model)
{
    <div id="@item.IdPublicidade" class="block-list-group">
        @{ Html.RenderPartial("_DetalheVisualizar", item); }
    </div>
}

@*Hack para, ao fazer o append dos dados da resposta, não ocorrer um erro devido à resposta estar vazia*@
@if (Model.Count() == 0) { <div></div> }